{
  "name": "formatted-blocks",
  "version": "1.0.0",
  "description": "Serialized platform independent content formatting - converts Markdown to structured FormattedBlocks format",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "keywords": [
    "markdown",
    "formatting",
    "text",
    "blocks",
    "unified",
    "remark"
  ],
  "files": [
    "lib/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf lib",
    "prebuild": "npm run clean",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "hast-to-hyperscript": "^10.0.0",
    "mdast-util-to-hast": "^13.0.0",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.0",
    "unist-util-inspect": "^8.0.0",
    "unist-util-map": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wookiehangover/formatted-blocks.git"
  },
  "bugs": {
    "url": "https://github.com/wookiehangover/formatted-blocks/issues"
  },
  "homepage": "https://github.com/wookiehangover/formatted-blocks#readme",
  "author": {
    "name": "Sam Breed",
    "email": "samuel.breed@gmail.com",
    "url": "https://github.com/wookiehangover"
  }
}
